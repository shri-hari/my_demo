
<% form_for @user_session ,:html =>{:class =>"form-horizontal"} do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :username%><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>