<div class= "container">
<div><p>
<h3><%= @title %></h3>
</p>
</div>
<div>
<table class = "table table-hover">
<thead>
<tr>
	<th><%= @title.singularize %> Name</th>
	<% unless request.url.include? "users" %>
	<th>City</th>
	<th>State</th>
	<th>Country</th>
	<% end %>
	<th>Email</th>
	<th>Action</th>
</tr>
</thead>
<tbody>
<% @users.each do |user| %>
<tr>
	<td><%= (user.username) %></td>
	<% unless request.url.include? "users" %>
	<td><%= (user.city) %></td>
	<td><%= (user.state) %></td>
	<td><%= (user.country) %></td>
	<% end %>
	<td><%= user.email %></td>
	<td><%= link_to "Delete",user_path(user),:method => :delete %>
</tr>
<% end %>
</tbody>
</table>
</div>
